{
  "name": "eleventy-theme-design2seo",
  "version": "1.0.0",
  "description": "11ty Starter Theme",
  "main": "index.njk",
  "scripts": {
    "start": "npm-run-all --parallel watch:*",
    "watch:start": "eleventy --serve",
    "build-production": "eleventy",
    "watch:sass": "sass --watch src/my-epic-css/index.scss:src/_includes/style.css",
    "epic-compile": "sass --style expanded --source-map --embed-sources --no-error-css --quiet src/my-epic-css/index.scss:src/_includes/style.css",
    "epic-build": "node ./node_modules/epic-css/src/epic-build.js",
    "epic-light": "node ./node_modules/epic-css/src/epic-light.js"
  },
  "keywords": [],
  "author": "Jeremy Faucher",
  "license": "ISC",
  "dependencies": {
    "bufferutil": "^4.0.5",
    "epic-css": "^0.4.0",
    "utf-8-validate": "^5.0.7"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-cache-assets": "^2.3.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.1.3",
    "clean-css": "^5.2.2",
    "npm-run-all": "^4.1.5",
    "sass": "^1.70.0",
    "terser": "^5.10.0"
  }
}
